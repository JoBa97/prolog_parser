list:		LOPEN lelements LCLOSE {}
		|	LOPEN lelements PIPE list {}
		|	LOPEN lelements PIPE id {}
		|	LOPEN LCLOSE {}
		;
		
lelements: 	lelement COMMA {}
			| lelement {}
			;
			
id_cons: id {}
	| CONST_ID {}
	;

id: VAR_ID {}
	| ANONYMOUS {}
	;
	
lelement:	id_const {}
			| number {}
			| list {}
			;
